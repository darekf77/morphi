{
  "files.exclude": {
    "node_modules": true,
    "tmp*": true,
    "dist*": true,
    "bundle*": true,
    "browser": true,
    "browser*": true,
    "module*": true,
    "backup": true,
    "module": true,
    "www": true,
    "Thumbs.db": true,
    ".DS_Store": true,
    "**/.DS_Store": true,
    "npm-debug.log*": true,
    ".sass-cache": true,
    ".sourcemaps": true,
    "src/tsconfig.app.json": true,
    "webpack.config.build.aot.js": true,
    "webpack.config.build.js": true,
    "webpack.config.common.js": true,
    "webpack.config.js": true,
    ".angular-cli.json": true,
    "src-for-stanalone": true,
    "components-for-stanalone": true,
    "src/assets/angular-lib": true,
    "src/assets/second-angular-lib": true,
    ".gitignore": true,
    ".npmignore": true,
    ".npmrc": true,
    ".babelrc": true,
    "docs": true,
    "logo.svg": true,
    "**/*.filetemplate": true,
    "**/tsconfig.*": true,
    "tslint.*": true,
    "index.*": true,
    "package-lock.json": true,
    "protractor.conf.js": true,
    "karma.conf.js": true,
    ".editorconfig": true
  },
  "workbench.colorCustomizations": {
    "activityBar.background": "#d30303",
    "activityBar.foreground": "#000000"
  },
  "editor.rulers": [
    100,
    120
  ],
  "html.format.wrapAttributes": "force-aligned",
  "workbench.colorTheme": "Default Light+",
  "tslint.enable": false
}